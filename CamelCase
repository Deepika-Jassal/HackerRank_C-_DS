int camelcase(string s) {
    int count=0;
    for(int i=0;i<s.length();i++){
        cout<<(int)s[i]<<" ";
        if(i==0){
            cout<<"000"; 
            if(97<=(int)s[i] && (int)s[i]<=122) 
                count++;
        }    
        else if(65<=(int)s[i] && (int)s[i]<=90){ 
            cout<<"else";
            count++;
        }
        cout<<count<<" ";
    }
    return count;
}
